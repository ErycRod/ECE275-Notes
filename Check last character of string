#include <iostream>
#include <string>

using namespace std;

int main()
{
    string userInput;
    
    cout<< "Enter a sentence: "<< endl;
    
    getline(cin, userInput);
    
    if(userInput.at(userInput.size()-1) == '?') //CHECKS IF LAST CHARACTER OF STRING IS '?'
        cout<< "A question.";
    
    else
        cout<< "Not a question.";
    
    return 0;
}
